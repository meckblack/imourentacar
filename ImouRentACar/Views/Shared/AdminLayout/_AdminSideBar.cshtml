<aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="../../dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
                <p>@ViewData["loggedinuserfullname"]</p>
                <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
            </div>
        </div>
        <!-- sidebar menu: : style can be found in sidebar.less -->
        <ul class="sidebar-menu" data-widget="tree">
            <li class="header">MAIN NAVIGATION</li>
            <li>
                <a asp-action="Dashboard" asp-controller="Admin">
                    <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="CarBrand">
                    <i class="fa fa-car"></i> <span>Car Brands</span>
                    <span class="pull-right-container">
                        <small class="label pull-right bg-blue">@ViewData["carbrandcounter"]</small>
                    </span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="Car">
                    <i class="fa fa-car"></i> <span>Cars</span>
                    <span class="pull-right-container">
                        <small class="label pull-right bg-blue">@ViewData["carcounter"]</small>
                    </span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="Landing">
                    <i class="fa fa-th"></i> <span>Landing Details</span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="Enquiry">
                    <i class="fa fa-th"></i> <span>Enquiries</span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="Price">
                    <i class="fa fa-th"></i> <span>Prices</span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="State">
                    <i class="fa fa-th"></i> <span>States</span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="Lga">
                    <i class="fa fa-th"></i> <span>Lgas</span>
                </a>
            </li>
            <li>
                <a asp-action="Index" asp-controller="Customer">
                    <i class="fa fa-th"></i> <span>Customers</span>
                </a>
            </li>
        </ul>
    </section>
    <!-- /.sidebar -->
    </aside>